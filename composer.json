{
  "name": "andreas-glaser/php-helpers",
  "description": "A simple php library of various helper functions and classes.",
  "keywords": [
    "php",
    "helper"
  ],
  "homepage": "https://github.com/andreas-glaser/php-helpers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Glaser",
      "email": "andreas.glaser@pm.me",
      "homepage": "https://andreas.glaser.me"
    }
  ],
  "require": {
    "php": "^7|^8.0",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^5",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "suggest": {},
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "AndreasGlaser\\Helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndreasGlaser\\Helpers\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1.x-dev"
    }
  }
}
