{
  "name": "andreas-glaser/php-helpers",
  "description": "A simple php library of various helper functions and classes.",
  "keywords": [
    "php",
    "helper"
  ],
  "homepage": "https://github.com/andreas-glaser/php-helpers",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Andreas Glaser",
      "email": "andreas.glaser@pm.me",
      "homepage": "https://github.com/andreas-glaser"
    }
  ],
  "require": {
    "php": "^8.2|^8.3|^8.4",
    "ext-mbstring": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3||^11",
    "friendsofphp/php-cs-fixer": "^3"
  },
  "suggest": {},
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "AndreasGlaser\\Helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AndreasGlaser\\Helpers\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.3.x-dev",
      "dev-dev": "1.4.x-dev"
    }
  }
}
